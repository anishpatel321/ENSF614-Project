package com.anish.FlightApp;

public class LineItem {

    private String item;
    private double cost;

    public LineItem() {
    }

    public LineItem(String item, double cost) {
       this.item = item;
       this.cost = cost;
    }

    public double getCost() {
    	return cost;
    }
    
    public void setCost(double cost) {
    	this.cost = cost;
    }
    
    public String getItem() {
        return item;
    }

    public void setItem(String item) {
        this.item =  item;
    }

}
